{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": [false],
        "forin": [false],
        "no-angle-bracket-type-assertion": [false],
        "no-bitwise": [false],
        "no-namespace": [false],
        "object-literal-key-quotes": [false],
        "object-literal-shorthand": [false],
        "object-literal-sort-keys": [false],
        "ordered-imports": [false],
        "prefer-for-of": [false]
    },
    "rulesDirectory": []
}
